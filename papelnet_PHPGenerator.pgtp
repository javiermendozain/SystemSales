    <DataSource name="venta_hoy" type="query">
      <PrimaryKeyFields>
        <Field name="n_factura"/>
      </PrimaryKeyFields>
      <SelectStatement>
SELECT   facturar_ventas.n_factura,
          sum(facturar_ventas.valor_total_pagar) as totalventa, 
        sum(facturar_ventas.utilidad_en_venta) as utlida       
  FROM   facturar_ventas
  WHERE  to_char(facturar_ventas.fecha_hora,'YYYY-MM-DD')=to_char(now(),'YYYY-MM-DD')
  
  GROUP BY facturar_ventas.n_factura      </SelectStatement>
    </DataSource>
