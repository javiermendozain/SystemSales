    <DataSource name="ciudad" type="query">
      <PrimaryKeyFields>
        <Field name="idciudad"/>
      </PrimaryKeyFields>
      <SelectStatement>
SELECT 
  ciudad.idciudad , departamento.iddepartamento, 
  concat(ciudad.nombre_ciudad  ,', ',departamento.nombre) as ciudad
  
FROM 
  public.ciudad, 
  public.departamento
WHERE 
  departamento.iddepartamento = ciudad.iddepartamento      </SelectStatement>
    </DataSource>
    <DataSource name="Departamento_multiselec" type="query">
      <PrimaryKeyFields>
        <Field name="iddepartamento"/>
      </PrimaryKeyFields>
      <SelectStatement>
SELECT 
  departamento.iddepartamento, 
  departamento.nombre
FROM 
  public.departamento
ORDER BY departamento.nombre      </SelectStatement>
    </DataSource>
    <DataSource name="product_compra" type="query">
      <PrimaryKeyFields>
        <Field name="idproducto"/>
      </PrimaryKeyFields>
      <SelectStatement>
SELECT 
 CONCAT( productos.nombre, ', ',
  caracteristicas.marca) as name,
  productos.idproducto
FROM 
  public.productos, 
  public.caracteristicas
WHERE 
  caracteristicas.idcaracter = productos.idcaracter      </SelectStatement>
    </DataSource>
    <DataSource name="producto_venta" type="query">
      <PrimaryKeyFields>
        <Field name="idproducto"/>
      </PrimaryKeyFields>
      <SelectStatement>
SELECT 
  productos.idproducto, 
  concat(productos.idproducto,' - ' ,
  to_char(productos.precio_venta,'LFM9,999,999')) as nombre
FROM 
  public.productos, 
  public.descuentos, 
  public.unidades, 
    public.caracteristicas
WHERE 
  descuentos.iddescuentos = productos.iddescuentos AND
  unidades.idunidad = productos.idunidad AND
    caracteristicas.idcaracter = productos.idcaracter AND
  productos.estado = 1      </SelectStatement>
    </DataSource>
    <DataSource name="Consultar_Precio_producto" type="query">
      <PrimaryKeyFields>
        <Field name="idproducto"/>
      </PrimaryKeyFields>
      <SelectStatement>
SELECT 
  productos.idproducto, 
  productos.nombre, 
  caracteristicas.marca,  
  to_char(productos.precio_venta,'LFM9,999,999') as precio, 
  productos.stock, 
  unidades.descripcion, 
  descuentos.descpcion_descuento
FROM 
  public.productos, 
  public.descuentos, 
  public.unidades, 
    public.caracteristicas
WHERE 
  descuentos.iddescuentos = productos.iddescuentos AND
  unidades.idunidad = productos.idunidad AND
    caracteristicas.idcaracter = productos.idcaracter AND
  productos.estado = 1      </SelectStatement>
    </DataSource>
    <DataSource name="all_facturas_ventas" type="query">
      <PrimaryKeyFields>
        <Field name="n_factura"/>
      </PrimaryKeyFields>
      <SelectStatement>
SELECT 
  clientes.nombre_completo, 
  facturar_ventas.n_factura, 
  facturar_ventas.valor_total_pagar, 
  facturar_ventas.abono, 
  facturar_ventas.tipo_de_venta, 
  to_char(facturar_ventas.fecha_hora, 'yyyy-MM-DD') as fecha_hora, 
  facturar_ventas.iva_total, 
  facturar_ventas.descuento_total, 
  facturar_ventas.saldo, 
  facturar_ventas.sub_total, 
  facturar_ventas.user_id,
  facturar_ventas.utilidad_en_venta
FROM 
  public.facturar_ventas, 
  public.clientes
WHERE 
  clientes.nit_cc = facturar_ventas.clientes_nit_cc
  ORDER BY facturar_ventas.fecha_hora desc      </SelectStatement>
    </DataSource>
    <DataSource name="utilidad_mensual" type="query">
      <PrimaryKeyFields>
        <Field name="mes"/>
      </PrimaryKeyFields>
      <SelectStatement>
SELECT 
  to_char(facturar_ventas.fecha_hora,'YYYY - Mon') as MES, 
  sum(facturar_ventas.utilidad_en_venta) as utilidad,
    sum(facturar_ventas.valor_total_pagar) as totalventas
FROM   public.facturar_ventas
  GROUP BY    to_char(facturar_ventas.fecha_hora,'YYYY - Mon')
  ORDER BY  to_char(facturar_ventas.fecha_hora,'YYYY - Mon') asc      </SelectStatement>
    </DataSource>
    <DataSource name="utilidad_x_ano" type="query">
      <PrimaryKeyFields>
        <Field name="anno"/>
      </PrimaryKeyFields>
      <SelectStatement>
SELECT 

  to_char(facturar_ventas.fecha_hora,'YYYY') as anno, 
  sum(facturar_ventas.utilidad_en_venta) as utilidad,
  sum(facturar_ventas.valor_total_pagar) as totalventas
FROM   public.facturar_ventas

GROUP BY  anno
ORDER BY anno desc      </SelectStatement>
    </DataSource>
    <DataSource name="inversion_utilidad" type="query">
      <PrimaryKeyFields>
        <Field name="n_prodctos"/>
      </PrimaryKeyFields>
      <SelectStatement>
SELECT 
  sum(stock*precio_venta) valor_bodega_actual, 
  sum(stock*precio_costo) as inversion_total, 
  count(idproducto) as n_prodctos,
  (sum(stock*precio_venta)- sum(stock*precio_costo)) as utilidad_total
FROM   productos      </SelectStatement>
    </DataSource>
